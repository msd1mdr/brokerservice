<?xml version="1.0" encoding="UTF-8" ?>
<definitions targetNamespace="http://www.mandiri.co.id/services/InvestorAcctBalance"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:tns="http://www.mandiri.co.id/services/InvestorAcctBalance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
             xmlns:tns1="http://www.mandiri.co.id/schema/InvestorAcctBalance"
             xmlns:soap12="http://schemas.xmlsoap.org/wsdl/soap12/">
  <types>
    <xsd:schema targetNamespace="http://www.mandiri.co.id/services/InvestorAcctBalance/types"
                elementFormDefault="qualified">
      <xsd:import schemaLocation="xsd/InvestorAcctBalance.xsd"
                  namespace="http://www.mandiri.co.id/schema/InvestorAcctBalance"/>
    </xsd:schema>
  </types>
  <message name="RequestMessage">
    <part name="part" element="tns1:InvestorAcctBalanceRequest"/>
  </message>
  <message name="ResponseMessage">
    <part name="part" element="tns1:InvestorAcctBalanceResponse"/>
  </message>
  <portType name="InvestorAcctBalanceService">
    <operation name="requestBalance">
      <input message="tns:RequestMessage"/>
      <output message="tns:ResponseMessage"/>
    </operation>
  </portType>
  <binding name="InvestorAcctBalanceServiceSOAP11Binding"
           type="tns:InvestorAcctBalanceService">
    <soap:binding style="document"
                  transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="requestBalance">
      <soap:operation style="document"
                      soapAction="http://www.mandiri.co.id/services/InvestorAcctBalance/requestBalance"/>
      <input>
        <soap:body use="literal" parts="part"/>
      </input>
      <output>
        <soap:body use="literal" parts="part"/>
      </output>
    </operation>
  </binding>
  <service name="InvestorAcctBalanceService">
    <port name="InvestorAcctBalanceServicePort"
          binding="tns:InvestorAcctBalanceServiceSOAP11Binding">
      <soap:address location="http://www.example.com"/>
    </port>
  </service>
</definitions>
